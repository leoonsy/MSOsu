<UserControl x:Class="MSOsu.View.MultipleCorrelationUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MSOsu.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" FontSize="14">
    <StackPanel Margin="5">
        <StackPanel Background="#FFF7F7F7">
            <WrapPanel Margin="5 5 5 0">
                <TextBlock Text="Коэффициент множественной корреляции для '"/>
                <TextBlock x:Name="tblHeaderName" TextDecorations="{x:Null}" />
                <TextBlock Text="': " FontSize="14"/>
                <TextBlock x:Name="tblMultipleCorrelation" FontWeight="Bold" />
            </WrapPanel>
            <Separator Margin="0 3 0 0" />
            <WrapPanel Margin="5 0 5 5">
                <TextBlock Text="Коэффициент детерминации для '" />
                <TextBlock Text="{Binding ElementName=tblHeaderName, Path=Text}" TextDecorations="{x:Null}" />
                <TextBlock Text="': " />
                <TextBlock x:Name="tblDetermination" FontWeight="Bold" />
            </WrapPanel>
        </StackPanel>
    </StackPanel>

</UserControl>
