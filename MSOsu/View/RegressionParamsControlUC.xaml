<UserControl x:Class="MSOsu.View.RegressionParamsControlUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MSOsu.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" FontSize="14">
    <StackPanel>
        <Border Padding="7" Background="#FFF7F7F7" >
            <TextBlock Text="Исключение параметров:" />
        </Border>
        <StackPanel x:Name="spMain" Margin="7 7 7 4" />
        <Button Padding="5" Content="Рассчитать" HorizontalAlignment="Left" Command="{Binding CalculateRegressionCommand}" Margin="7 0 7 7" />
    </StackPanel>
</UserControl>
