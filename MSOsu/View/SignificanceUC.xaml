<UserControl x:Class="MSOsu.View.SignificanceUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MSOsu.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Border Padding="3 5 5 5" Background="#FFF7F7F7">
                <TextBlock x:Name="tblHeader1" FontSize="14" Margin="4 0 0 0" />
            </Border>
            <ContentPresenter x:Name="cpTable1" />
            <Border Padding="3 5 5 5" Background="#FFF7F7F7">
                <TextBlock x:Name="tblHeader2" FontSize="14" Margin="5 0 0 0" />
            </Border>
            <ContentPresenter x:Name="cpTable2" Margin="0 0 0 5" />
        </StackPanel>
    </Grid>
</UserControl>
